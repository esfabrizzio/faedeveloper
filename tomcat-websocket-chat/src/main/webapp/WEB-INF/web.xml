<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">

    <description>Chat websocket example</description>
    <display-name>Chat websocket example</display-name>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
    <servlet>
    	<servlet-name>Qp2tonigh</servlet-name>
    	<servlet-class>q.p2.to.n.Qp2tonight</servlet-class>
    	<load-on-startup>-1</load-on-startup>
    </servlet>
    
    <servlet-mapping>
    	<servlet-name>Qp2tonigh</servlet-name>
    	<url-pattern>/qp2ton</url-pattern>
    </servlet-mapping>
</web-app>
